{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"jest": true
	},
	"extends": "airbnb-base",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-console": [
			"error",
			{
				"allow": [
					"warn",
					"error",
					"log",
					"assert"
				]
			}
		],
		"no-tabs": [
			"error",
			{
				"allowIndentationTabs": true
			}
		],
		"indent": [
			2,
			"tab"
		],
		"quotes": 0,
		"linebreak-style": 0,
		"no-plusplus": 0,
		"no-confusing-arrow": 0,
		"no-unused-expressions": 0,
		"no-param-reassign": 0,
		"no-restricted-syntax": 0,
		"no-nested-ternary": 0,
		"strict": 0,
		"object-curly-newline": 0,
		"no-extra-semi": 0,
		"global-require": 0,
		"radix": 0,
		"camelcase": 0,
		"class-methods-use-this": 0,
		"consistent-return": 0,
		"quote-props": 0,
		"import/no-extraneous-dependencies": 0,
		"no-unused-vars": 0,
		"no-undef": 0
	}
}
